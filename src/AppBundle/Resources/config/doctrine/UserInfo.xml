<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\UserInfo" table="user_info" repository-class="AppBundle\Entity\UserInfoRepository">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="first_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstSurname" type="string" column="first_surname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secondSurname" type="string" column="second_surname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateOfBirth" type="date" column="date_of_birth" nullable="false"/>
    <field name="placeOfBirth" type="string" column="place_of_birth" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gender" type="string" column="gender" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="postCode" type="string" column="post_code" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rfc" type="string" column="rfc" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="curp" type="string" column="curp" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
  </entity>
</doctrine-mapping>
